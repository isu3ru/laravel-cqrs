{
    "name": "isu3ru/laravel-cqrs",
    "description": "A proper CQRS utility library for Laravel. Originally from serrexlabs/laravel-cqrs",
    "authors": [
        {
            "name": "Isuru Ranawaka",
            "email": "isuru@mybooking.lk"
        }
    ],
    "require": {
        "league/fractal": "^0.17.0",
        "illuminate/console": "^6.4",
        "illuminate/container": "^6.4",
        "illuminate/contracts": "^6.4",
        "illuminate/http": "^6.4",
        "illuminate/support": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "Isu3ru\\Cqrs\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Isu3ru\\Cqrs\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
