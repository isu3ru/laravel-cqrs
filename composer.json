{
    "name": "isu3ru/laravel-cqrs",
    "description": "A proper CQRS utility library for Laravel. Originally from serrexlabs/laravel-cqrs",
    "authors": [
        {
            "name": "Isuru Ranawaka",
            "email": "isuru@mybooking.lk"
        }
    ],
    "require": {
        "php": "^8.2",
        "league/fractal": "^0.20",
        "illuminate/console": "^12.0",
        "illuminate/container": "^12.0",
        "illuminate/http": "^12.0",
        "illuminate/support": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "Isu3ru\\Cqrs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Isu3ru\\Cqrs\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "phpunit/phpunit": "^11.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Isu3ru\\Cqrs\\ServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
